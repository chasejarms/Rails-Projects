<!DOCTYPE html>
<html>
<head>
  <title>AuthenticationPractice</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if current_user %>
  Welcome, <%= current_user.username %>
  <%= button_to "Log out", session_url, method: :delete %>
<% end %>

<% if flash[:errors] %>
  <%= flash[:errors] %>
<% end %>



<%= yield %>

</body>
</html>
